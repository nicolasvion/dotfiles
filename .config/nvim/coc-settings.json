{
  "explorer.width": 30,
  "explorer.sources": [
    {
      "name": "buffer",
      "expand": true
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.file.autoReveal": true,
  "explorer.icon.enableVimDevicons": true,
  "explorer.openAction.relativePath": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon": {
  "expanded": " ",
  "collapsed": " "
  },
  "explorer.file.showHiddenFiles": false,
  "explorer.file.hiddenRules": {
    "patternMatches": [
      "^\\.idea$",
      "^\\.DS_Store$",
      "^\\.git$"
    ]
  },
  "explorer.git.icon.status.unmodified": "  ",
  "explorer.git.icon.status.mixed": "",
  "explorer.git.icon.status.added": "●",
  "explorer.git.icon.status.modified": " ",
  "explorer.git.icon.status.deleted": " ",
  "explorer.git.icon.status.untracked": "",
  "explorer.git.icon.rootStatus.conflicted": "⇡⇣",
  "explorer.git.icon.rootStatus.behind": "",
  "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5]",
  "explorer.buffer.root.template": "[icon & 1] Opened Files",
  "explorer.file.root.template": "[icon & 1] Project ([root])",
  "explorer.keyMappings.global": {
    "<space>": "toggleSelection",
    "s": "open:vsplit",
    "r": "rename",
    "c": "copyFile",
    "p": "pasteFile",
    "d": "delete",
    "a": "addFile",
    "A": "addDirectory",
    "J": "expand:Recursive",
    "K": "collapse:Recursive",
    "<cr>": "open"
  }
}
